<configuration>
  <appSettings>
    <!-- update these with your BotId, Microsoft App Id and your Microsoft App Password-->
    <add key="BotId" value="__BotId__" />
    <add key="MicrosoftAppId" value="__MicrosoftAppId__" />
    <add key="MicrosoftAppPassword" value="__MicrosoftAppPassword__" />
    <!-- LINE settings -->
    <add key="ChannelSecret" value="__ChannelSecret__" />
    <add key="ChannelToken" value="__ChannelToken__" />
    <!-- Direct Line -->
    <add key="DirectLineSecret" value="__DirectLineSecret__" />
    <!-- AAD Auth v1 settings -->
    <add key="ActiveDirectory.Mode" value="v1" />
    <add key="ActiveDirectory.ResourceId" value="https://graph.windows.net/" />
    <add key="ActiveDirectory.EndpointUrl" value="https://login.microsoftonline.com" />
    <add key="ActiveDirectory.Tenant" value="__Tenant__" />
    <add key="ActiveDirectory.ClientId" value="__ClientId__" />
    <add key="ActiveDirectory.ClientSecret" value="__ClientSecret__" />    
    <add key="ActiveDirectory.RedirectUrl" value="__RedirectUrl__" />
    <!--<add key="ActiveDirectory.RedirectUrl" value="http://localhost:3979/api/OAuthCallback" />-->
    <add key="D365Url" value="__D365Url__" />
    <add key="O365Url" value="https://graph.microsoft.com" />    
    <add key="StorageConnectionString" value="__StorageConnectionString__" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="4.6" />
    <httpRuntime targetFramework="4.6" />
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="default.htm" />
      </files>
    </defaultDocument>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
